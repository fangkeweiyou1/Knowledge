<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.module_banner">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        >

        <activity android:name=".activity.BannerActivity" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

        <receiver android:name=".receiver.BannerBroadCastReciver">
            <intent-filter>
                <action android:name="open_banneractivity"/>
            </intent-filter>
        </receiver>

    </application>

</manifest>
