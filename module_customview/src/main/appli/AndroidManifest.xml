<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.module_customview">

    <application
        android:icon="@mipmap/ic_launcher"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >

        <activity android:name=".activity.CustomViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.RecyclerHomeActivity"/>
        <activity android:name=".activity.UDLRRecyclerActivity"/>
        <activity android:name=".activity.PartRecyclerActivity"/>
        <activity android:name=".activity.DialogBasicActvitity"/>
        <activity android:name=".activity.TextBasicActivity"/>

    </application>

</manifest>
